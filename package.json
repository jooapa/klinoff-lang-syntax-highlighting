{
  "name": "klinoff-lang-syntax-highlighting",
  "displayName": "klinoff-lang Syntax Highlighting",
  "description": "Have you wanted to write klinoff-lang in peace and fast, well look no further, a groundbreaking technology of syntax highlighting from klinoff-lang",
  "version": "0.0.3",
  "publisher": "Jooapa",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/jooapa/klinoff-lang-syntax-highlighting"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "kln",
      "aliases": ["klinoff-lang", "kln"],
      "extensions": [".kln", ".nöff"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "kln",
      "scopeName": "source.kln",
      "path": "./syntaxes/klinoff-lang.tmLanguage.json"
    }]
  },
  "fileAssociations": {
    "*.kln": "images/icon.png",
    "*.nöff": "images/icon.png"
  }
}